service doomrl-server
{
        socket_type     = stream
        protocol        = tcp
        wait            = no
        user            = doomrl
        group           = nogroup
        server          = /usr/sbin/in.telnetd
        server_args     = -h -L /var/lib/doomrl/doomrl-server
        port            = 3666
        type            = UNLISTED
}
