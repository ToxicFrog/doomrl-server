service doomrl-server
{
        socket_type     = stream
        protocol        = tcp
        wait            = no
        user            = doomrl
        group           = nogroup
        server          = /usr/sbin/in.telnetd
        server_args     = -h -L /usr/share/doomrl-server/etc/doomrl-server.telnetd-wrapper
        port            = 3666
        type            = UNLISTED
}
